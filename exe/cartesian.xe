
  Cartesian := new

  (record sum (A:Type) (B:Type): Type :=
         (left: A)
         (right: B))

    (data unit: Type :=
         (make: () → unit))

  (record prod (A:Type) (B:Type): Type :=
         (pr1: A)
         (pr2: B))

  (record sigma (A: Type) (B: A → Type): Type :=
         (pr1: A)
         (pr2: B pr1))

    (data empty: Type := ())
