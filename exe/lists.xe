
   Lists := new

             (data list (A:Type): Type :=
                  (nil: () → list A)
                  (cons: A → list A → list A))

           (record lists (A,B: Type): Type :=
                  (len: list A → integer)
                  ('++': list A → list A → list A)
                  (map: (A → B) → (list A → list B))
                  (filter: (A → bool) → (list A → list A)))



