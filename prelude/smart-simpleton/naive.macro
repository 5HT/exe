with (#Nat.Base) in
Nat.Naive (A : Alg.Ob) :=
new ( minP : Alg.P A := new
        ( Ok a := record (ind (Ok : Alg.P A) : Ok a) )
        ( Z'.ind       Ok := Ok.Z' )
        ( S'.ind el ok Ok := Ok.S' el (ok Ok) ) )
    ( minOb : Alg.Ob := SmallSigma A minP )
    ( induc (Ok : Alg.P A) (n : minOb) : Ok n.el := n.ok Ok )
