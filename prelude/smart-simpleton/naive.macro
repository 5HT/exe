with (#Nat.Base) in
Nat.Naive (A : Alg.Ob) :=
new ( minP : Alg.P A := new
        ( Ok a := record (ind (p : Alg.P A) : p.Ok a) )
        ( Z'.ind       p := p.Z' )
        ( S'.ind el ok p := p.S' el (ok p) ) )
    ( minA : Alg.Ob := SmallSigma A minP )
    ( induc (p : Alg.P A) (n : minA) : P (n.el) := n.ok p )
