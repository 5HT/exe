with (#Nat.Base) in
Nat.Naive (alg : Alg.Ob) :=
new ( naiveP : Alg.Predicate alg := new
        ( Ok el := record ( induc (p : Alg.Predicate alg) : p.Ok el ) )
        ( ZeroOk.induc       p := p.ZeroOk )
        ( SuccOk.induc el ok p := p.SuccOk el (ok p) ) )
    ( minimal : Alg.Ob := SmallSigma alg naiveP )
    ( induc (p : Alg.Predicate alg) (n : minimal.Carrier)
        : P (n.el) := n.ok p )
