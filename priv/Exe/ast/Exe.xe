
    data Macro: * :=
         (record: Record → Exe)
         (data: Data → Exe)
         (let: Let → Exe)
         (case: Case → Exe)

    data Effect: * :=
         (receive: Case → Exe)
         (spawn: Process → Exe)
         (send: * → Process → Exe)
         (try: Exception → Exe)
         (raise: Exception → Exe)
         (write: File → Exe)
         (read: File → Exe)

    data Exe: * :=
         (om: Om → Exe)
         (core: Macro → Exe)
         (eff: Effect → Exe)
